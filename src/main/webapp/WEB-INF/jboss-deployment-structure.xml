<?xml version="1.0" encoding="UTF-8"?>
<!-- This fixes resteasy jaxb incompatibilities. -->
<jboss-deployment-structure>  
    <deployment>  
        <exclusions>  
            <module name="org.jboss.resteasy.resteasy-jaxb-provider" />  
       </exclusions>  
    </deployment>  
</jboss-deployment-structure> 
