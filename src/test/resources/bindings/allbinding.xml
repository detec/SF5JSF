 <!-- https://jax-ws.java.net/nonav/2.1.2/docs/customizations.html
 Official docs: The root jaxws:bindings element may contain child jaxws:bindings elements. 
 In this case the child jaxws:bindings element must carry an XPath expression in the node attribute to refer to the WSDL node it customizes.
 
  -->

<jaxws:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:jaxws="http://java.sun.com/xml/ns/jaxws" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    version="2.1">
    
    <jaxws:bindings wsdlLocation="../wsdl/SatellitesService.wsdl"
		node="definitions[@targetNamespace='http://sf5.openbox.org/satelliteservice/1.0']">
		
		
		<!-- 		node="wsdl:definitions/wsdl:types/xsd:schema[@targetNamespace='http://sf5.openbox.org/satelliteservice/1.0']"> -->
<!-- 		node="definitions/types/xsd:schema[@import namespace='http://sf5.openbox.org/satelliteservice/1.0']"> -->
		<jaxb:schemaBindings>
			<jaxb:package name="org.openbox.sf5.jaxws.client.gen.satellite" />
		</jaxb:schemaBindings>
		<jaxws:package name="org.openbox.sf5.jaxws.client.gen.satellite" />
			
	</jaxws:bindings>
	
	<jaxws:bindings
		wsdlLocation="../wsdl/SettingsService.wsdl"
		node="definitions/types/xsd:schema[@import&#x20;namespace='http://sf5.openbox.org/settingsservice/1.0']">
		<jaxb:schemaBindings>
			<jaxb:package name="org.openbox.sf5.jaxws.client.gen.setting" />
		</jaxb:schemaBindings>
		<jaxws:package name="org.openbox.sf5.jaxws.client.gen.setting" />
	</jaxws:bindings>
	
	<jaxws:bindings
		wsdlLocation="../wsdl/TranspondersService.wsdl"
		node="wsdl:definitions/wsdl:types/xsd:schema[@targetNamespace='http://sf5.openbox.org/transpondersservice/1.0']">
		<jaxb:schemaBindings>
			<jaxb:package name="org.openbox.sf5.jaxws.client.gen.transponder" />
		</jaxb:schemaBindings>
		<jaxws:package name="org.openbox.sf5.jaxws.client.gen.transponder" />
	</jaxws:bindings>
	
	<jaxws:bindings
		wsdlLocation="../wsdl/UsersService.wsdl"
		node="wsdl:definitions/wsdl:types/xsd:schema[@targetNamespace='http://sf5.openbox.org/usersservice/1.0']">
		<jaxb:schemaBindings>
			<jaxb:package name="org.openbox.sf5.jaxws.client.gen.user" />
		</jaxb:schemaBindings>
		<jaxws:package name="org.openbox.sf5.jaxws.client.gen.user" />
	</jaxws:bindings>
	

</jaxws:bindings>